  "publicApiSurface": [
    "fzf - Main interactive finder command",
    "--bind - Key/event binding configuration",
    "--preview - Preview window command",
    "--height - Non-fullscreen display mode",
    "--tmux - Tmux popup mode",
    "--multi - Multi-select mode",
    "--filter - Non-interactive filter mode",
    "--ansi - ANSI color code processing",
    "--color - Color scheme customization",
    "--query - Initial query string",
    "--select-1 - Auto-select if one match",
    "--exit-0 - Auto-exit if no match",
    "--delimiter - Field delimiter",
    "--nth - Field index for search",
    "--with-nth - Field index for display",
    "CTRL-T - Paste selected files (shell binding)",
    "CTRL-R - Paste selected command from history (shell binding)",
    "ALT-C - Change to selected directory (shell binding)",
    "FZF_DEFAULT_COMMAND - Default input command",
    "FZF_DEFAULT_OPTS - Default options"
  ]
